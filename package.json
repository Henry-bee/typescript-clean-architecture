{
  "name": "typescript-clean-architecture",
  "version": "1.0.1",
  "description": "Typescript clean architecture implementantion.",
  "license": "MIT",
  "scripts": {
    "start": "gulp",
    "test": "jest --forceExit",
    "lint": "gulp tslint",
    "build": "gulp build",
    "generate-doc": "gulp doc"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.spec.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "ajv": "^5.1.5",
    "awilix": "^2.3.0",
    "body-parser": "^1.17.2",
    "camelcase": "^4.1.0",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/camelcase": "^0.0.30",
    "@types/debug": "0.0.29",
    "@types/express": "4.0.33",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.87",
    "@types/morgan": "^1.7.32",
    "@types/node": "6.0.46",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-debug": "^3.1.0",
    "gulp-file-cache": "^0.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sequence": "^0.4.6",
    "gulp-sourcemap": "^1.0.1",
    "gulp-tslint": "^8.1.1",
    "gulp-typedoc": "^2.1.1",
    "gulp-typescript": "^3.1.7",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typedoc": "^0.9.0"
  }
}